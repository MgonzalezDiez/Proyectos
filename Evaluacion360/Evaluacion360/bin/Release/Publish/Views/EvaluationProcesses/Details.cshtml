@model Evaluacion360.Models.ViewModels.EvaluationProcessViewModel

@{
    ViewBag.Title = "Detalle Proceso Evaluación";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Detalle Proceso Evaluación</title>
    <!-- meta tags -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Pooled Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript">

        addEventListener("load", function()
            { setTimeout(hideURLbar, 0); },
            false);
            function hideURLbar()
            { window.scrollTo(0,1); }
    </script>

    <!-- Bootstrap Core CSS -->
    <link href="~/Css-Pool/bootstrap.min.css" rel="stylesheet" type='text/css' />
    <!-- Custom CSS -->
    <link href="~/Css-Pool/style2.css" rel='stylesheet' type='text/css' />
    <link href="~/Css-Modernizer/style.css" rel="stylesheet" />
    <!-- Graph CSS -->
    <link href="~/Css-Pool/font-awesome.css" rel="stylesheet" />
    <link href="~/Css/jquery-ui.css" rel="stylesheet" />


    <!-- jQuery -->
    <script src="~/js/jquery-2.1.4.min.js"></script>
    <!-- //jQuery -->
    <link href='//fonts.googleapis.com/css?family=Roboto:700,500,300,100italic,100,400' rel='stylesheet' type='text/css' />
    <link href='//fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css' />
    <!-- lined-icons -->
    <link href="~/Css/icon-font.min.css" rel="stylesheet" type='text/css' />
    <!-- //lined-icons -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>

</head>
<body>
    <div class="panel-body" , style="background-color:#fff">
        <form form="role" class="form-horizontal" action="Delete" method="post">
            <div class="notification_header" style="text-align:center">
                <h1 class="center-block">Procesos de Evaluación</h1>
            </div>
            <br />
            <div form="role" class="form-group has-feedback">
                @Html.AntiForgeryToken()
                <label class="col-md-3 control-label">Código de Proceso</label>
                <div class="col-md-8">
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-cogs"></i></span>
                        @Html.EditorFor(model => model.Codigo_Proceso, new { htmlAttributes = new { @readonly = "readonly", type = "text", @class = "form-control1" } })

                    </div>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Descripción</label>
                <div class="col-md-8">
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-discourse"></i></span>
                        @Html.EditorFor(model => model.Nombre_Proceso, new { htmlAttributes = new { @readonly = "readonly", type = "text", @class = "form-control1" } })
                        <div class="clearfix"></div>
                    </div>
                </div>
            </div>

            <div class="form-group">
                <label class="col-md-3 control-label">Año de Proceso</label>
                <div class="col-md-8">
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                        @Html.EditorFor(model => model.Ano_Proceso, new { htmlAttributes = new { @readonly = "readonly", type = "text", @class = "form-control1" } })
                        <div class="clearfix"></div>
                    </div>
                </div>
            </div>

            <div class="form-group">
                <label class="col-md-3 control-label">Mes Proceso</label>
                <div class="col-md-8">
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                        @Html.EditorFor(model => model.Mes_Proceso, new { htmlAttributes = new { @readonly = "readonly", type = "text", @class = "form-control1" } })
                    </div>
                </div>
            </div>

            <div class="form-group">
                <label class="col-md-3 control-label">Retroalimentación</label>
                <div class="col-md-8">
                    <div class="input-group">
                        <span class="input-group-addon"><i class="far fa-comments"></i></span>
                        @Html.EditorFor(model => model.Retroalimentacion, new { htmlAttributes = new { @readonly = "readonly", type = "text", @class = "form-control1" } })
                    </div>
                </div>
            </div>

            <div class="form-group">
                <label class="col-md-3 control-label">Estado Evaluación</label>
                <div class="col-md-8">
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-question-circle"></i></span>
                        @Html.DropDownListFor(model => model.Estado_PE, (SelectList)ViewBag.StateEval, "Estado", new { @readonly = "readonly", type = "text", @class = "form-control1" })
                    </div>
                </div>
            </div>

            <br />
            <div class="panel-footer">
                <label class="col-md-3 control-label"></label>
                <div class="col-md-8">
                    <div class="form-actions no-color">
                        <input type="button" class="btn btn-success btn-sm" onclick="location.href='@Url.Action("List", "EvaluationProcesses")'" value="Volver a la Lista" />
                    </div>
                </div>
            </div>
        </form>
    </div>
    <script src="~/js/Utiles.js"></script>
</body>
</html>



