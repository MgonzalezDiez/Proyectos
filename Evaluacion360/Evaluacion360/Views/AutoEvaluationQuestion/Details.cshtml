@model Evaluacion360.Models.ViewModels.AutoEvaluationQuestionListModel

@{
    ViewBag.Title = "Detalles Auto Evaluación";
    Layout = "";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Detalle de Auto Evaluación</title>
    <!-- meta tags -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Pooled Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript">

        addEventListener("load", function()
            { setTimeout(hideURLbar, 0); },
            false);
            function hideURLbar()
            { window.scrollTo(0,1); }
    </script>

    <!-- Bootstrap Core CSS -->
    <link href="~/Css-Pool/bootstrap.min.css" rel="stylesheet" type='text/css' />

    <!-- Custom CSS -->
    <link href="~/Css-Pool/style2.css" rel='stylesheet' type='text/css' />
    <link href="~/Css-Modernizer/style.css" rel="stylesheet" />

    <!-- Graph CSS -->
    <link href="~/Css-Pool/font-awesome.css" rel="stylesheet" />
    <link href="~/Css/jquery-ui.css" rel="stylesheet" />
    <!-- jQuery -->
    <script src="~/js/jquery-2.1.4.min.js"></script>
    <!-- //jQuery -->
    <link href='//fonts.googleapis.com/css?family=Roboto:700,500,300,100italic,100,400' rel='stylesheet' type='text/css' />
    <link href='//fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css' />
    <!-- lined-icons -->
    <link href="~/Css/icon-font.min.css" rel="stylesheet" type='text/css' />
    <!-- //lined-icons -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>

</head>
<body>
    <div class="panel-body" , style="background-color:#fff">
        <div class="notification_header" style="text-align:center">
            <h1 class="center-block">Detalle de Cargos</h1>
        </div>
        <br />
        <form form="role" class="form-horizontal">
            @Html.AntiForgeryToken()
            <div class="form-group has-feedback">
                <label class="col-md-3 control-label ">Número de Evaluación</label>
                <div class="col-md-8">
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-user-secret"></i></span>
                        @Html.EditorFor(model => model.Numero_Evaluacion, new { htmlAttributes = new { @readonly = "readonly", type = "text", @class = "form-control1" } })
                    </div>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Código de Proceso</label>
                <div class="col-md-8">
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-user"></i></span>
                        @Html.EditorFor(model => model.Codigo_Proceso, new { htmlAttributes = new { type = "text", disabled = "disabled", @class = "form-control1", @placeholder = "Nombre de Cargo" } })
                        <div class="clearfix"></div>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Sección</label>
                <div class="col-md-8">
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-user-plus"></i></span>
                        @Html.DropDownListFor(model => model.Codigo_Seccion, (SelectList)ViewBag.Section, "Seleccione Sección", new { type = "text", @class = "form-control1" })
                        <div class="clearfix"></div>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Pregunta</label>
                <div class="col-md-8">
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-user-plus"></i></span>
                        @Html.EditorFor(model => model.Numero_Pregunta, new { htmlAttributes = new { type = "text", @class = "form-control1" } })
                        <div class="clearfix"></div>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Nota</label>
                <div class="col-md-8">
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-user-plus"></i></span>
                        @Html.EditorFor(model => model.Nota, new { htmlAttributes = new { @id = "decimal", type = "text", @class = "form-control1" } })
                        <div class="clearfix"></div>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Estado</label>
                <div class="col-md-8">
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-user-plus"></i></span>
                        @Html.EditorFor(model => model.Estado_AE, new { htmlAttributes = new { type = "text", @readonly = "readonly", @class = "form-control1", @placeholder = "Estado" } })
                        <div class="clearfix"></div>
                    </div>
                </div>
            </div>

            <div class="panel-footer">
                <div class="form-group">
                    <label class="col-md-3 control-label"></label>
                    <div class="col-md-8">
                        <input onclick="location.href='@Url.Action("List", "SelfEvaluationQuestion")'" type="button" value="Volver a la Lista" class="btn btn-success btn-sm" />
                    </div>
                </div>
            </div>
        </form>
    </div>
</body>
</html>



